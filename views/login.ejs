<!DOCTYPE html>
<html>
<head>
  <%- include('partials/head.ejs') %>
</head>
<body>

  <%- include('partials/nav.ejs') %>
<div class="container" style="padding-top: 7%;padding-bottom: 13%;">

    <div class="columns is-mobile">
        <div class="column is-half is-offset-one-quarter">
            <h1 class="title" style="text-align: center;">Sign In With <%= title%> Account</h1>
            <div class="field">
                <label class="label">Email</label>

                <input class="input" type="email" placeholder="Email">
        
              </div>
              <div class="field">
                <label class="label">Password</label>

                 <input class="input" type="password" placeholder="Password">
           
          
              </div>
              <a href="/">Forgot Password?</a>
              <button class="button is-fullwidth is-primary is-outlined">Login</button>
              <p>Doesn't have a <%= title%> account?</p>
              <div class="buttons">
                <button class="button is-primary">Sign Up</button>
              </div>

        </div>
      
      </div>



</div>
<%- include('partials/footer.ejs') %>
</body>
</html>